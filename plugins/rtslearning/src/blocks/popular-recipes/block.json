    "apiVersion": 2,
    "name": "rtslearning/popular-recipes",
    "title": "Popular Recipes",
    "category": "text",
    "description": "Adds a list of highly rated recipes",
    "textdomain": "rtslearning",
    "attributes": {
      "title": {
        "type": "string",
        "default": "Popular Recipes"
      },
      "count": {
